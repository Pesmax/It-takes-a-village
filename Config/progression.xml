			<effect_description level="3"  desc_key="attITAVRank03Desc" long_desc_key="attITAVRank03LongDesc"/>
			<effect_description level="4"  desc_key="attITAVRank04Desc" long_desc_key="attITAVRank04LongDesc"/>
			<effect_description level="5"  desc_key="attITAVRank05Desc" long_desc_key="attITAVRank05LongDesc"/>
			<effect_description level="6"  desc_key="attITAVRank06Desc" long_desc_key="attITAVRank06LongDesc"/>
			<effect_description level="7"  desc_key="attITAVRank07Desc" long_desc_key="attITAVRank07LongDesc"/>
