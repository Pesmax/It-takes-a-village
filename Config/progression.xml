		<level_requirements level="10"><requirement name="PlayerLevel" operation="GTE" value="1" desc_key="reqGenericPoints03"/></level_requirements>
		<effect_group>
			<triggered_effect trigger="onPerkLevelChanged" action="AddBuff" buff="buffPerkAbilityUpdate"/>
			<passive_effect name="ElectricalTrapXP" operation="base_set" level="8,9,10" value="0.2,0.35,0.5"/>
			<passive_effect name="LootGamestage" operation="perc_add" level="6,10" value=".05,.25"/>
				<!-- LootGamestage only counts for containers that you open. It does not affect party loot GS. -->
			<passive_effect name="ScavengingTime" operation="perc_subtract" level="6,7,8,9,10" value=".1,.2,.4,.6,.8"/>
			<effect_description level="1"  desc_key="attITAVRank01Desc" long_desc_key="attITAVRank01LongDesc"/>
